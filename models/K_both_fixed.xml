<model name="K_both_fixed">
	<variable name="V" value="-60">
		<tmp current="1" name="I_A">$(nA)*$(nA)*$(nA)*$(nA)*$(hA)*$(gA) * ($(V)-$(EK))</tmp>
		<tmp current="1" name="I_K">$(nK)*$(nK)*$(nK)*$(nK)*$(hK)*$(gK) * ($(V)-$(EK))</tmp>
		<tmp current="1" name="I_l">$(gl) * ($(V)-$(El))</tmp>
		<dYdt>(Isyn - I_A - I_K - I_l)/$(C)</dYdt>
		<range min="-150" max="100" />
	</variable>
	<variable name="nA" value="0.02">
		<tmp name="_inf">1.0/(1.0 + exp(($(nA_mid)-$(V))/$(nA_slope)))</tmp>
		<tmp name="tau_">$(taunA_min) + $(taunA_max)/(exp(($(taunA_off)+$(V))/$(taunA_slope1))+exp(($(taunA_off)+$(V))/$(taunA_slope2)))</tmp>
		<dYdt>((_inf-$(nA))/tau_)</dYdt>
		<range min="0" max="1" />
	</variable>
	<variable name="hA" value="0.99">
		<tmp name="_inf">$(hA_0) + (1.0-$(hA_0))/(1.0 + exp(($(hA_mid)-$(V))/$(hA_slope)))</tmp>
		<tmp name="tau_">$(tauhA_min) + $(tauhA_max)/(1.0 + exp(($(tauhA_mid)+$(V))/$(tauhA_slope)))</tmp>
		<dYdt>((_inf-$(hA))/tau_)</dYdt>
		<range min="0" max="1" />
	</variable>
	<variable name="nK" value="0.02">
		<tmp name="_inf">1.0/(1.0 + exp(($(nK_mid)-$(V))/$(nK_slope)))</tmp>
		<tmp name="tau_">$(taunK_min) + $(taunK_max)/(exp(($(taunK_off)+$(V))/$(taunK_slope1))+exp(($(taunK_off)+$(V))/$(taunK_slope2)))</tmp>
		<dYdt>((_inf-$(nK))/tau_)</dYdt>
		<range min="0" max="1" />
	</variable>
	<variable name="hK" value="0.99">
		<tmp name="_inf">$(hK_0) + (1.0-$(hK_0))/(1.0 + exp(($(hK_mid)-$(V))/$(hK_slope)))</tmp>
		<tmp name="tau_">$(tauhK_min) + $(tauhK_max)/(1.0 + exp(($(tauhK_mid)-$(V))/$(tauhK_slope)))</tmp>
		<dYdt>((_inf-$(hK))/tau_)</dYdt>
		<range min="0" max="1" />
	</variable>

	<adjustableParam name="gA" value="60">
		<range min="10" max="200"/>
		<perturbation type="*" rate="0.05"/>
	</adjustableParam>
	<adjustableParam name="gK" value="60">
		<range min="10" max="200"/>
		<perturbation type="*" rate="0.05"/>
	</adjustableParam>
	<adjustableParam name="EK" value="-80">
		<range min="-100" max="-50"/>
		<perturbation type="+" rate="1"/>
	</adjustableParam>

	<adjustableParam name="gl" value="0.1">
		<range min="0.0001" max="10"/>
		<perturbation type="*" rate="0.01"/>
	</adjustableParam>
	<adjustableParam name="El" value="-10">
		<range min="-30" max="10"/>
		<perturbation type="+" rate="1"/>
	</adjustableParam>

	<adjustableParam name="C" value="150">
		<range min="50" max="200"/>
		<perturbation type="*" rate="0.01"/>
	</adjustableParam>

	<parameter name="nA_mid" value="-55" />
	<parameter name="nA_slope" value="20" />
	<parameter name="taunA_min" value="2" />
	<parameter name="taunA_max" value="102" />
	<parameter name="taunA_off" value="45" />
	<parameter name="taunA_slope1" value="6" />
	<parameter name="taunA_slope2" value="-40" />
	<parameter name="hA_0" value="0.05" />
	<parameter name="hA_mid" value="-48" />
	<parameter name="hA_slope" value="-5" />
	<parameter name="tauhA_min" value="35" />
	<parameter name="tauhA_max" value="10000" />
	<parameter name="tauhA_mid" value="72" />
	<parameter name="tauhA_slope" value="7" />

	<parameter name="nK_mid" value="-24" />
	<parameter name="nK_slope" value="10.5" />
	<parameter name="taunK_min" value="5" />
	<parameter name="taunK_max" value="110" />
	<parameter name="taunK_off" value="36" />
	<parameter name="taunK_slope1" value="18" />
	<parameter name="taunK_slope2" value="-17" />
	<parameter name="hK_0" value="0.2" />
	<parameter name="hK_mid" value="-26" />
	<parameter name="hK_slope" value="-6" />
	<parameter name="tauhK_min" value="400" />
	<parameter name="tauhK_max" value="3000" />
	<parameter name="tauhK_mid" value="-80" />
	<parameter name="tauhK_slope" value="15" />
</model>
