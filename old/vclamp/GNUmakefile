INCLUDE_FLAGS	= -I$(GENN_PATH)/lib/include 
CXXFLAGS	= $(INCLUDE_FLAGS) -std=c++11 -Wall -Winline -O3 -g

all: generate run

clean:
	$(RM) generate run
